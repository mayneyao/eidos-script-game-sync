{
    "id": "game-sync",
    "name": "Game Sync",
    "description": "Sync game data from rawig.io",
    "version": "0.0.1",
    "main": "src/main.js",
    "inputJSONSchema": {
        "type": "object",
        "properties": {
            "username": {
                "type": "string"
            }
        },
        "required": [
            "username"
        ]
    },
    "outputJSONSchema": {
        "type": "object",
        "properties": {
            "ok": {
                "type": "boolean"
            }
        },
        "required": [
            "ok"
        ]
    },
    "tables": [
        {
            "name": "game",
            "fields": [
                {
                    "name": "name",
                    "type": "string"
                },
                {
                    "name": "slug",
                    "type": "string"
                },
                {
                    "name": "background_image",
                    "type": "string"
                },
                {
                    "name": "released",
                    "type": "string"
                }
            ]
        }
    ]
}